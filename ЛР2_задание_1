//Добавление библиотеки
#include <stdio.h>
int main() {
	// Добавление переменных 
	int a, b;
	// Добавление циклов 
	printf("The value is = ");
	for (a = 1; a <= 9; a++)
	for (b = 0; b <= 9; b++)
		// Добавление условия 
			if (a * a + b * b == 3 * a * b + 1 && 10 * a + b == 7 * (a + b) + 6) {
				// Вывод значения в конслоли 
				printf("%d\n", 10 * a + b);
			}
	
	return 0;
}
